doctype html
html
	head
		title= title
		link(href='//fonts.googleapis.com/css?family=Jura:400,600', rel='stylesheet', type='text/css')
		link(rel='stylesheet', href='/css/jeep.css')

	body
		block navbar
			nav.navbar.navbar-inverse(role='navigation')
				.navbar-header
					button.navbar-toggle(type='button', data-toggle="collapse", data-target="#bs-example-navbar-collapse-1")
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href="#") Dropout-Jeep
				.collapse.navbar-collapse#navblock1
					ul.nav.navbar-nav
						li.active: a(href='#') Latest
						li: a(href='/queue') Queue
					form.navbar-form.navbar-left(role="search")
						.form-group
							input.form-control(type="text", placeholder="Search")
						button.btn.btn-default(type="submit") Go
					ul.nav.navbar-nav.navbar-right
						li: a(href="/signup") Sign up
						li: a(href='/signin') Sign in
		block content
